# Production Environment Variables for Frontend

# API Configuration - UPDATE THESE AFTER BACKEND DEPLOYMENT
VITE_API_URL=https://your-backend-url.railway.app
VITE_API_TIMEOUT=15000

# App Configuration
VITE_APP_NAME=Anti-Procrastination Coach
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered productivity app to overcome procrastination and achieve life goals

# Authentication
VITE_JWT_STORAGE_KEY=auth_token
VITE_REFRESH_TOKEN_KEY=refresh_token

# Features
VITE_ENABLE_AI_COACH=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SOCIAL_FEATURES=true

# Production Settings
VITE_DEBUG_MODE=false
VITE_MOCK_API=false
